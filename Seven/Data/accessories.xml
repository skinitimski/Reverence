    </accessory>
    <accessory>
        <name>Choco Feather</name>
        <desc>Dexterity +10</desc>
        <attach>c.DexterityBonus = c.DexterityBonus + 10</attach>
        <detach>c.DexterityBonus = c.DexterityBonus - 10</detach>
    </accessory>
    <accessory>
        <name>Earring</name>
        <desc>Magic +10</desc>
        <attach>c.MagicBonus = c.MagicBonus + 10</attach>
        <detach>c.MagicBonus = c.MagicBonus - 10</detach>
    </accessory>
    <accessory>
        <name>Talisman</name>
        <desc>Spirit +10</desc>
        <attach>c.SpiritBonus = c.SpiritBonus + 10</attach>
