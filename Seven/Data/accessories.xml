        <attach>c.LuckBonus = c.LuckBonus + 10</attach>
        <detach>c.LuckBonus = c.LuckBonus - 10</detach>
    </accessory>
    <accessory>
        <name>Championship Belt</name>
        <desc>Strength&amp;Vitality +30</desc>
        <attach>c.StrengthBonus = c.StrengthBonus + 30; c.VitalityBonus = c.VitalityBonus + 30</attach>
        <detach>c.StrengthBonus = c.StrengthBonus - 30; c.VitalityBonus = c.VitalityBonus - 30</detach>
    </accessory>
    <accessory>
        <name>Circlet</name>
        <desc>Magic&amp;Spirit +30</desc>
        <attach>c.MagicBonus = c.MagicBonus + 30; c.SpiritBonus = c.SpiritBonus + 30</attach>
        <detach>c.MagicBonus = c.MagicBonus - 30; c.SpiritBonus = c.SpiritBonus - 30</detach>
    </accessory>
    <accessory>
        <name>Touph Ring</name>
        <desc>Vitality&amp;Spirit +50</desc>
        <attach>c.VitalityBonus = c.VitalityBonus + 50; c.SpiritBonus = c.SpiritBonus + 50</attach>
        <detach>c.VitalityBonus = c.VitalityBonus - 50; c.SpiritBonus = c.SpiritBonus - 50</detach>
    </accessory>
    <accessory>
        <name>Fire Ring</name>
        <desc>Nullifies [Fire] attacks</desc>
        <attach>c:AddVoid(Element.Fire)</attach>
        <detach>c:RemoveVoid(Element.Fire)</detach>
    </accessory>
    <accessory>
        <name>Ice Ring</name>
        <desc>Nullifies [Ice] attacks</desc>
        <attach>c:AddVoid(Element.Ice)</attach>
        <detach>c:RemoveVoid(Element.Ice)</detach>
    </accessory>
    <accessory>
        <name>Bolt Ring</name>
        <desc>Nullifies [Lightning] attacks</desc>
        <attach>c:AddVoid(Element.Lightning)</attach>
        <detach>c:RemoveVoid(Element.Lightning)</detach>
    </accessory>
    <accessory>
        <name>Tetra Elemental</name>
        <desc>Absorbs [Fire/Ice/Earth/Lightning] attacks</desc>
        <attach>
            c:AddAbsorb(Element.Fire)
            c:AddAbsorb(Element.Ice)
            c:AddAbsorb(Element.Earth)
            c:AddAbsorb(Element.Lightning)
        </attach>
        <detach>
            c:RemoveAbsorb(Element.Fire)
            c:RemoveAbsorb(Element.Ice)
            c:RemoveAbsorb(Element.Earth)
            c:RemoveAbsorb(Element.Lightning)
        </detach>
    </accessory>
    <accessory>
        <name>Poison Ring</name>
        <desc>Absorbs [Poison] attacks</desc>
        <attach>c:AddAbsorb(Element.Poison)</attach>
        <detach>c:RemoveAbsorb(Element.Poison)</detach>
    </accessory>
    <accessory>
        <name>Water Ring</name>
        <desc>Absorbs [Water] attacks</desc>
        <attach>c:AddAbsorb(Element.Water)</attach>
        <detach>c:RemoveAbsorb(Element.Water)</detach>
    </accessory>
</accessories>