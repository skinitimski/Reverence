<!-- ********* ACCESSORIES ********* -->
<accessories>
    <accessory>
        <name>Power Wrist</name>
        <desc>Strength +10</desc>
        <attach>c.StrengthBonus = c.StrengthBonus + 10</attach>
        <detach>c.StrengthBonus = c.StrengthBonus - 10</detach>
    </accessory>
    <accessory>
        <name>Protect Vest</name>
        <desc>Vitality +10</desc>
        <attach>c.VitalityBonus = c.VitalityBonus + 10</attach>
