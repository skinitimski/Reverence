            <immunity>Berserk</immunity>
        </immunities>
        <attacks>        
            <attack id="Bodyblow">         
                <type>Physical</type>
                <power>16</power>
                <atkp>100</atkp>
                <target>Combatant</target>
                <element>Hit</element>
                <hidden>true</hidden>
            </attack>
        </attacks>
        <ai>
            <setup>count = math.random(0,Party.PARTY_SIZE - 1)</setup>
            <main>
                local a = BattleState.Allies[count]
                
                if not a.Death then
                    self:UseAttack("Bodyblow", a)
                end
                
                count = (count + 1) % Party.PARTY_SIZE
            </main>
            <counter>
                if self.HP &lt;= self.MaxHP * 4 / 3 and math.random(0,2) == 0 then
                    local a = self.LastAttacker
                    self:UseMagicSpell("Fire", a)
                end   
            </counter>
            <confuAttack>Bodyblow</confuAttack>
            <berserkAttack>Bodyblow</berserkAttack>
        </ai>
    </enemy>
